package com.Inner.com;
class Outer{
		int x=20;
class Inner{
int y=30;
public void Innerdisplay() {
	System.out.println(x+","+y);
}

	}

public void outerDisplay() {
	Inner i =new Inner();
	i.Innerdisplay();
}	
}
public class Test {//nestedInner
public static void main(String[]args) {
	Outer o= new Outer();
	o.outerDisplay();
}
}
