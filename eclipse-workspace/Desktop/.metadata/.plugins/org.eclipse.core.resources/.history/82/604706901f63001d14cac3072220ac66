package com.Inheritance.com;
class Parent {
	public Parent() {
		System.out.println("Parent constructor");
	}
}
class Child extends Parent
{
	public Child() {
		System.out.println("Child constructore");
	}
}
public class ConVsIn {

	public static void main(String[] args) {
		
		Parent p=new Child();
	}

}
