package com.Inheritance.com;
class Super{
	public void display() {
		System.out.println("Super class Display");
	}
}
class Sub extends Super{
	public void display() {
		System.out.println("Sub class Display");
	}
}
public class Test12 {
public static void main(String[]args) {
	Super sp=new Super();
	sp.display();
	Sub sb=new Sub();
	sb.display();
}
}
